<meta name="viewport" content="width=device-width, initial-scale=1"/>
<meta name="description" content="The CSS implementation of Adobe's Spectrum design system"/>
<meta charset="utf-8"/>
<link rel="icon" type="image/png" href="favicon.png"/>
{# <link rel="stylesheet" type="text/css" href="/css/prism/prism.css" data-prism="data-prism"/> #}
<link rel="stylesheet" type="text/css" href="@spectrum-css/vars/spectrum-global.css"/>
<link rel="stylesheet" type="text/css" href="@spectrum-css/vars/spectrum-darkest.css"/>
<link rel="stylesheet" type="text/css" href="@spectrum-css/vars/spectrum-dark.css"/>
<link rel="stylesheet" type="text/css" href="@spectrum-css/vars/spectrum-light.css"/>
<link rel="stylesheet" type="text/css" href="@spectrum-css/vars/spectrum-medium.css"/>
<link rel="stylesheet" type="text/css" href="@spectrum-css/vars/spectrum-large.css"/>
<link rel="stylesheet" type="text/css" href="@spectrum-css/tokens/index.css"/>
{% set dependencyOrder = [
  'icon',
  'statuslight',
  'link',
  'page',
  'site',
  'typography',
  'tooltip',
  'sidenav',
  'actionbutton',
  'button',
  'textfield',
  'clearbutton',
  'search',
  'menu',
  'fieldlabel',
  'picker',
  'popover',
  'underlay',
  'card',
  'divider',
  'illustratedmessage',
  'accordion',
  'table'

] %}
{% asyncEach dep in dependencyOrder %}
  <link rel="stylesheet" type="text/css" href='@spectrum-css/{{ dep }}/index-vars.css' data-dependency={{ dep }} />
{% endeach %}
<link rel="stylesheet" type="text/css" href="/css/site.css"/>
<link rel="stylesheet" type="text/css" href="/css/docs.css"/>

{# prism css #}
    <link href="https://unpkg.com/prismjs@1.20.0/themes/prism.css" rel="stylesheet">
    {# <link href="https://unpkg.com/prismjs@1.20.0/themes/prism-dark.css" rel="stylesheet"> #}

{% if not process.env.VRT %}
    <link
        rel="stylesheet"
        type="text/css"
        href="//wwwimages2.adobe.com/etc/beagle/public/globalnav/adobe-globalnav/latest/adobe-globalnav.min.css"/>
{% endif %}
<script src="/js/loadicons/index.js"></script>
<script src="/js/focus-ring-polyfill/index.js"></script>
<script src="/js/lunr/lunr.js"></script>
<script src="/js/typekit.js"></script>
