{
  "name": "spectrum-css",
  "version": "2.0.0-1",
  "scripts": {
    "build": "gulp build && npm run docs",
    "docs": "topdoc",
    "watch:docs": "onchange \"docs-template/\" -- npm run docs",
    "livereload": "livereload \"dist\"/",
    "dev": "npm-run-all --parallel watch:* livereload",
    "tdv": "topdoc --version",
    "copy-dependencies": "sync -rv --exclude=*.pug docs-template dist/reference/docs"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "del": "^2.2.0",
    "gulp": "^3.9.1",
    "gulp-bump": "^2.1.0",
    "gulp-concat": "^2.6.0",
    "gulp-connect": "^4.0.0",
    "gulp-git": "^1.7.2",
    "gulp-postcss": "^6.1.1",
    "gulp-rename": "^1.2.2",
    "gulp-stylus": "^2.4.0",
    "gulp-util": "^3.0.7",
    "gulp-wrap": "^0.13.0",
    "inquirer": "^1.0.3",
    "livereload": "^0.6.0",
    "npm-run-all": "^3.1.1",
    "onchange": "^3.0.2",
    "run-sequence": "^1.2.1",
    "semver": "^5.1.0",
    "spectrum-icons": "git+ssh://git@git.corp.adobe.com:Spectrum/icons.git#1.0.0-pre.4",
    "spectrum-palette": "git+ssh://git@git.corp.adobe.com:Spectrum/palette.git#1.0.0-pre.13",
    "stylus": "^0.54.5",
    "svg-stylus": "git+https://github.com/lazd/svg-stylus.git",
    "topdoc": "^1.0.2"
  }
}
