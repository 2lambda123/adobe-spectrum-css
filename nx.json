{
	"npmScope": "@spectrum-css",
	"tasksRunnerOptions": {
		"default": {
			"runner": "nx/tasks-runners/default",
			"options": {
				"cacheableOperations": ["build"]
			}
		}
	},
	"targetDefaults": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["{workspaceRoot}/*.css"],
			"outputs": ["{workspaceRoot}/dist"]
		}
	},
	"includedScripts": ["build"]
}
