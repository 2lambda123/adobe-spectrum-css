{
	"name": "well",
	"tags": ["component"],
	"targets": {
		"clean": {
			"executor": "nx:run-commands",
			"options": {
				"cwd": "components/well",
				"command": "rimraf dist"
			}
		},
		"build": {
			"inputs": ["legacystyles"],
			"executor": "nx:run-commands",
			"options": {
				"command": "gulp --gulpfile components/well/gulpfile.js --cwd components/well build"
			}
		}
	}
}
