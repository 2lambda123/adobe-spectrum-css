{
	"name": "progresscircle",
	"tags": ["component"],
	"targets": {
		"clean": {
			"executor": "nx:run-commands",
			"options": {
				"command": "rimraf dist"
			}
		},
		"build": {
			"inputs": [
				"{workspaceRoot}/index.css",
				"{workspaceRoot}/skin.css",
				"{workspaceRoot}/themes/spectrum.css",
				"{workspaceRoot}/themes/express.css"
			],
			"executor": "nx:run-commands",
			"options": {
				"command": "gulp"
			}
		},
		"test": {
			"executor": "nx:run-commands",
			"options": {
				"command": "nx test:scope @spectrum-css/preview progresscircle"
			}
		}
	}
}
