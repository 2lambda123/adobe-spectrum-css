{
	"name": "typography",
	"tags": ["component"],
	"targets": {
		"clean": {
			"executor": "nx:run-commands",
			"options": {
				"cwd": "components/typography",
				"command": "rimraf dist"
			}
		},
		"build": {
			"executor": "nx:run-commands",
			"options": {
				"command": "gulp --gulpfile components/typography/gulpfile.js --cwd components/typography build"
			}
		}
	}
}
