{
	"name": "accordion",
	"tags": ["components"],
	"targets": {
		"build": {
			"dependsOn": ["^build"],
			"executor": "nx:run-commands",
			"options": {
				"cwd": "components/accordion",
				"command": "gulp"
			}
		},
		"clean": {
			"executor": "nx:run-commands",
			"options": {
				"cwd": "components/accordion",
				"command": "rimraf dist"
			}
		}
	}
}
