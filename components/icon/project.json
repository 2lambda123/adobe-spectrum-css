{
	"name": "icon",
	"tags": ["component"],
	"targets": {
		"clean": {
			"executor": "nx:run-commands",
			"options": {
				"cwd": "components/icon",
				"command": "rimraf dist"
			}
		},
		"build": {
			"inputs": [
				"{workspaceRoot}/index.css"
			],
			"executor": "nx:run-commands",
			"options": {
				"cwd": "components/icon",
				"command": "gulp"
			}
		},
		"test": {
			"executor": "nx:run-commands",
			"options": {
				"command": "nx test:scope @spectrum-css/preview icon"
			}
		}
	}
}
