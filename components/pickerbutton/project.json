{
	"name": "pickerbutton",
	"tags": ["component"],
	"targets": {
		"clean": {
			"executor": "nx:run-commands",
			"options": {
				"command": "rimraf dist"
			}
		},
		"build": {
			"inputs": [
				"{workspaceRoot}/index.css",
				"{workspaceRoot}/pickerbutton-generated.css"
			],
			"executor": "nx:run-commands",
			"options": {
				"command": "gulp"
			}
		},
		"test": {
			"executor": "nx:run-commands",
			"options": {
				"command": "nx test:scope @spectrum-css/preview pickerbutton"
			}
		}
	}
}
