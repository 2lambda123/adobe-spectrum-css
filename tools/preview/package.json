{
  "name": "@spectrum-css/preview",
  "version": "3.1.3",
  "description": "A Spectrum CSS preview",
  "license": "Apache-2.0",
  "author": "Adobe",
  "homepage": "https://opensource.adobe.com/spectrum-css/preview",
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/spectrum-css.git",
    "directory": "tools/preview"
  },
  "bugs": {
    "url": "https://github.com/adobe/spectrum-css/issues"
  },
  "main": "main.js",
  "scripts": {
    "build": "storybook build --quiet --config-dir .",
    "dev": "run-p dev:storybook watch",
    "dev:storybook": "storybook dev --quiet --config-dir .",
    "new": "nx run @spectrum-css/generator:new story",
    "start": "WATCH_MODE=true yarn dev",
    "test": "chromatic --build-script-name build --junit-report",
    "test:scope": "yarn test --only-story-names",
    "watch": "nx watch --projects tag:components -- nx run \\$NX_PROJECT_NAME:build"
  },
  "devDependencies": {
    "@adobe/spectrum-css-workflow-icons": "^1.2.1",
    "@babel/core": "^7.22.1",
    "@etchteam/storybook-addon-status": "^4.2.4",
    "@storybook/addon-a11y": "^7.0.20",
    "@storybook/addon-actions": "^7.0.20",
    "@storybook/addon-console": "^2.0.0",
    "@storybook/addon-docs": "^7.0.20",
    "@storybook/addon-essentials": "^7.0.20",
    "@storybook/addons": "^7.0.20",
    "@storybook/api": "^7.0.20",
    "@storybook/blocks": "^7.0.20",
    "@storybook/client-api": "^7.0.20",
    "@storybook/components": "^7.0.20",
    "@storybook/core-events": "^7.0.20",
    "@storybook/manager-api": "^7.0.20",
    "@storybook/preview-api": "^7.0.20",
    "@storybook/theming": "^7.0.20",
    "@storybook/web-components-webpack5": "^7.0.20",
    "@whitespace/storybook-addon-html": "^5.1.6",
    "chromatic": "^6.19.9",
    "file-loader": "6.2.0",
    "lit": "^2.7.6",
    "lodash-es": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "nx": "^16.4.1",
    "postcss": "^7.0.36",
    "postcss-loader": "^7.3.3",
    "postcss-prefix-selector": "^1.16.0",
    "postcss-selector-replace": "^1.0.2",
    "postcss-warn-cleaner": "^0.1.9",
    "prettier": "^3.0.0",
    "raw-loader": "^4.0.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-syntax-highlighter": "^15.5.0",
    "rimraf": "^5.0.1",
    "source-map-loader": "^4.0.1",
    "storybook": "^7.0.26",
    "style-loader": "3.3.3",
    "typescript": "^5.1.3",
    "webpack": "^5.87.0"
  }
