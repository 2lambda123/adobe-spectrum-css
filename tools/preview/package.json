{
  "name": "@spectrum-css/preview",
  "version": "3.0.26",
  "description": "A Spectrum CSS preview",
  "license": "Apache-2.0",
  "author": "Adobe",
  "homepage": "https://opensource.adobe.com/spectrum-css/preview",
  "main": "main.js",
  "scripts": {
    "build": "yarn storybook",
    "clean": "rimraf storybook-static",
    "dev": "yarn start --port 3001 --ci",
    "new": "yarn workspace @spectrum-css/generator new story",
    "start": "cross-env NODE_ENV=development WATCH_MODE=true storybook dev --quiet --config-dir .",
    "storybook": "storybook build --quiet --config-dir .",
    "test": "chromatic --build-script-name build --junit-report",
    "test:scope": "yarn test --only-story-names"
  },
  "dependencies": {
    "@adobe/spectrum-css-workflow-icons": "^1.2.1",
    "@spectrum-css/expressvars": "^3.0.8",
    "@spectrum-css/tokens": "^10.1.2",
    "@spectrum-css/vars": "^9.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-env": "^7.21.5",
    "@babel/runtime": "^7.21.5",
    "@etchteam/storybook-addon-status": "^4.2.4",
    "@spectrum-css/component-builder": "^4.0.12",
    "@storybook/addon-a11y": "^7.0.20",
    "@storybook/addon-actions": "^7.0.20",
    "@storybook/addon-console": "^1.2.3",
    "@storybook/addon-docs": "^7.0.20",
    "@storybook/addon-essentials": "^7.0.20",
    "@storybook/api": "^7.0.20",
    "@storybook/client-api": "^7.0.20",
    "@storybook/components": "^7.0.20",
    "@storybook/core-events": "^7.0.20",
    "@storybook/manager-api": "^7.0.20",
    "@storybook/preview-api": "^7.0.20",
    "@storybook/theming": "^7.0.20",
    "@storybook/web-components-webpack5": "^7.0.20",
    "@whitespace/storybook-addon-html": "^5.1.4",
    "chromatic": "^6.18.2",
    "file-loader": "6.2.0",
    "lit": "^2.7.4",
    "lit-html": "^2.7.4",
    "lodash-es": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.36",
    "postcss-class-prefix": "^0.3.0",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^4.0.0",
    "postcss-prefix-selector": "^1.16.0",
    "postcss-selector-replace": "^1.0.2",
    "postcss-warn-cleaner": "^0.1.9",
    "prettier": "^2.8.8",
    "raw-loader": "^4.0.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-syntax-highlighter": "^15.5.0",
    "rimraf": "^5.0.1",
    "source-map-loader": "^1.0.0",
    "storybook": "^7.0.20",
    "style-loader": "3.3.3",
    "webpack": "^5.83.1",
    "webpack-merge": "^5.8.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "entry",
          "corejs": 2
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "nx": {
    "namedInputs": {
      "core": [
        "{workspaceRoot}/assets",
        "{workspaceRoot}/decorators",
        "{workspaceRoot}/*.js",
        "{workspaceRoot}/*.html",
        "{projectRoot}/components/*/dist"
      ],
      "output": [
        "{workspaceRoot}/storybook-static"
      ]
    },
    "targets": {
      "build": {
        "dependsOn": [
          "^build"
        ],
        "inputs": [
          "core"
        ],
        "outputs": [
          "output"
        ]
      },
      "clean": {
        "inputs": [
          "output"
        ]
      },
      "start": {
        "dependsOn": [
          "^build"
        ],
        "inputs": [
          "core"
        ]
      },
      "storybook": {
        "dependsOn": [
          "^build"
        ],
        "inputs": [
          "core"
        ],
        "outputs": [
          "output"
        ]
      },
      "dev": {
        "dependsOn": [
          "^build"
        ],
        "inputs": [
          "core"
        ]
      }
    },
    "implicitDependencies": [
      "@spectrum-css/*",
      "!@spectrum-css/generator",
      "!@spectrum-css/documentation"
    ],
    "includedScripts": [
      "clean",
      "build",
      "storybook",
      "start",
      "dev",
      "test",
      "test:scope"
    ]
  }
}
