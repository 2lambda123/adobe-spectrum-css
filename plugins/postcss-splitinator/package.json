{
  "private": true,
  "name": "postcss-splitinator",
  "version": "1.0.2",
  "description": "Turn container queryies into the var hack",
  "license": "Apache-2.0",
  "author": "Adobe",
  "main": "index.js",
  "scripts": {
    "test": "ava"
  },
  "dependencies": {
    "postcss": "^7.0.32"
  },
  "publishConfig": {
    "access": "public"
  },
  "nx": {
    "tags": [
      "components"
    ],
    "targets": {
      "build": {
        "dependsOn": [
          "clean",
          "^build"
        ],
        "executor": "nx:run-commands",
        "options": {
          "cwd": "components/accordion",
          "command": "gulp"
        }
      },
      "clean": {
        "executor": "nx:run-commands",
        "options": {
          "command": "rimraf {workspaceRoot}/dist"
        }
      }
    }
  }
}
